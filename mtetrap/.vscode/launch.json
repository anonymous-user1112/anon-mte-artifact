{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/android-unpackbootimg/unpackbootimg",
            "args": [
                "-o",
                "${workspaceFolder}/unpacked_image",
                "-i",
                "${workspaceFolder}/init_boot.img"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "extract-symvers.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/extract-symvers-ng/extract-symvers.py",
            "args": [
                "-B", "0xffff8000",
                "-k", "5.15",
                "-b", "64",
                "./unpacked_image/boot_uncompressed",
            ]
        }
    ]
}